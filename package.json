    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "gapi-script": "^1.2.0",
    "googleapis": "^105.0.0",
